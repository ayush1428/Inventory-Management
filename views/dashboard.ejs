                <span class="ms-auto">
                  <span class="right-icon">
                    <i class="bi bi-chevron"></i>
                  </span>
                </span>
              </button>
              
              <button class="nav-link px-3 sidebar-link abc" onclick="redirectToReturnEquipments()">
                <span class="me-2"><i class="bi bi-box mt-2"></i></span>
                <span>Return Equipments</span>
                <span class="ms-auto">
                  <span class="right-icon">
                    <i class="bi bi-chevron"></i>
                  </span>
                </span>
              </button>
              <button class="nav-link px-3 sidebar-link abc" onclick="redirectToAssignEquipments()">
                <span class="me-2"><i class="bi bi-box mt-2"></i></span>
                <span>Assign Equipments</span>
                <span class="ms-auto">
                  <span class="right-icon">
                    <i class="bi bi-chevron"></i>
                  </span>
                </span>
